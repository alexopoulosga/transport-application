                                                                                                                                                                                                                                                                                                                                                                                                                   descartesupem-app-457929343ebe/src/modules/auth/components/login/login.component.ts                 0000664 0000000 0000000 00000003300 13304473505 0030345 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit } from '@angular/core';
import { Router } from '@angular/router';
import { FormBuilder, FormGroup, Validators } from '@angular/forms';

import { AuthStore } from '@stores/auth.store';
import { UserStore } from '@stores/user.store';

import { StorageService } from '@services/storage.service';
import { ToasterService } from '@services/toaster.service';

import { Credentials } from '@models/credentials.model';

@Component({
    selector: 'md-login',
    templateUrl: './login.component.html',
    styleUrls: ['./login.component.scss']
})
export class LoginComponent {

    loginForm: FormGroup;
    credentials: Credentials;

    authFailed = false;

    constructor(private builder: FormBuilder,
                private authStore: AuthStore,
                private userStore: UserStore,
                private storageSvc: StorageService,
                private toasterSvc: ToasterService,
                private router: Router) {
        this._initForm();
    }

    authentificate() {
        if (this.loginForm.valid) {
            this.credentials = this.loginForm.value;
            this.authStore.authentificateUser(this.credentials)
                .subscribe(
                    data => {
                        this.toasterSvc.success('Bienvenue sur l\'application MobilitŒì¬© Descartes');
                        this.router.navigate(['ride']);
                    },
                    error => this.toasterSvc.error('Combinaison invalide')
                );
        }
    }

    private _initForm() {
        this.loginForm = this.builder.group({
            email: ['', [Validators.required, Validators.email]],
            password: ['', Validators.required]
        });
    }

}
