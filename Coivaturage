                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  descartesupem-app-457929343ebe/src/modules/app/components/app/app.component.ts                      0000664 0000000 0000000 00000002535 13304473505 0027315 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit, ViewContainerRef } from '@angular/core';
import { Router, NavigationEnd, NavigationStart } from '@angular/router';

import { ToastsManager } from 'ng2-toastr/ng2-toastr';
import { swipe } from '@enums/swipe.eum';

import { DataService } from '@services/data.service';

@Component({
    selector: 'md-root',
    templateUrl: './app.component.html',
    styleUrls: ['./app.component.scss']
})

export class AppComponent implements OnInit {
    hideNavBar: boolean;
    route: string;

    autorizedRoute: string[];

    constructor(private router: Router, public toastr: ToastsManager, vRef: ViewContainerRef, private dataSvc: DataService) {
        this.autorizedRoute = ['user', 'ride', 'reservation', 'cgu', 'chat', 'notification'];
        this.toastr.setRootViewContainerRef(vRef);
    }
    
    ngOnInit() {
        this.router.events.subscribe(event => {
            this.hideNavBar = true;
            if (event instanceof NavigationEnd) {
                this.autorizedRoute.forEach(route => {
                    if (event.url.includes(route)) {
                        this.hideNavBar = false;
                    }
                });
                this.route = event.url.split('/').reverse()[0];
                if (this.route === '') {
                    this.route = 'home';
                }
            }
        });
    }
}

