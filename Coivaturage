                                                                                                                                                                                                                                                                                                                                                                                                                   descartesupem-app-457929343ebe/src/modules/app/components/error/error.component.ts                  0000664 0000000 0000000 00000002232 13304473505 0030231 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit } from '@angular/core';
import { ActivatedRoute } from '@angular/router';

import { Observable } from 'rxjs/Observable';
import { Subscription } from 'rxjs/Subscription';
import 'rxjs/add/operator/map';

import { HttpErrors } from '@enums/httpErrors.enum';

import { StorageService } from '@services/storage.service';

@Component({
    selector: 'md-error',
    templateUrl: './error.component.html',
    styleUrls: ['./error.component.scss']
})

export class ErrorComponent implements OnInit {

    errors: any;
    errorCode = 404;
    errorText: string;
    authorized = false;

    constructor(private route: ActivatedRoute, private storageSvc: StorageService) {
        this.errors = HttpErrors;
    }

    ngOnInit() {
        const param: Observable<number> = this.route.params.map(p => p.code);
        param.subscribe((code: number) => {
            this.errorCode = this.errors[code] === undefined ? 404 : +code;
            this.errorText = this.errors[this.errorCode];
            if (this.errorCode === 403) {
                this.storageSvc.removeItem('token');
                this.authorized = true;
            }
        });
    }

}
