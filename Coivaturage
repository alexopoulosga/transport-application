                                                                                                                                                                                                                                                                                                                                                                  descartesupem-app-457929343ebe/src/modules/chat/components/conversation/conversation.component.ts   0000664 0000000 0000000 00000005742 13304473505 0033343 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit, OnDestroy } from '@angular/core';
import { ActivatedRoute, Router } from '@angular/router';
import { FormGroup, FormBuilder, FormControl, Validators } from '@angular/forms';
import { Location } from '@angular/common';
import { Observable } from 'rxjs/Observable';
import { Subscription } from 'rxjs/Subscription';

import { Chat } from '@models/chat.model';
import { Message } from '@models/message.model';
import { MessageSend } from '@models/messageSend.model';

import { ChatStore } from '@stores/chat.store';

import { ToasterService } from '@services/toaster.service';

@Component({
    selector: 'md-conversation',
    templateUrl: './conversation.component.html',
    styleUrls: ['./conversation.component.scss']
})
export class ConversationComponent implements OnInit, OnDestroy {
    loaded = false;

    id: number;
    chat: Chat;
    messages: Message[];
    receiver: any;
    lastDate: any;

    maxLength = 1000;

    messageForm: FormGroup;

    subscription: Subscription;

    constructor(private location: Location,
                private route: ActivatedRoute,
                private router: Router,
                private builder: FormBuilder,
                private toaster: ToasterService,
                private chatStore: ChatStore) {
        this._initForm();
    }

    ngOnInit() {
        const id: Observable<number> = this.route.params.map(param => param.id);
        id.subscribe((parameter: number) => {
            if (parameter) {
                this.id = +parameter;
                this.getConversation(this.id);
            }
        });
    }

    getConversation(id: number) {
        this.messageForm.reset();
        this.subscription = this.chatStore.getChat(id).subscribe(
            data => {
                this.chat = data;
                this.messages = this.chat.messages;
                this.receiver = this.chat.receiver;
                this.lastDate = this.messages.length ? this.messages[this.messages.length - 1].date : Date.now();
            },
            error => this.router.navigate(['error', error.status]),
            () => this.loaded = true
        );
    }

    send(e) {
      e.preventDefault();
      if (this.messageForm.valid) {
          const message: MessageSend = this._prepareSend();
          this.chatStore.sendMessage(message).subscribe(
              success => this.getConversation(this.id),
              error => this.toaster.error('Erreur lors de l\'envoi du message')
          );
      }
    }

    goBack() {
        this.location.back();
    }

    ngOnDestroy() {
        this.subscription.unsubscribe();
    }

    private _prepareSend(): MessageSend {
        const message: MessageSend = <MessageSend>{};
        message.chat_id = this.id;
        message.content = this.messageForm.value.message;
        return message;
    }

    private _initForm() {
        this.messageForm = this.builder.group({
            message: ['', [Validators.required, Validators.maxLength(this.maxLength)]]
        });
    }

}
