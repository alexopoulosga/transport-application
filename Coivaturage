                                                                                                                                                                                                                                                                                                                                                                                                            descartesupem-app-457929343ebe/src/modules/ride/components/search/search.component.ts               0000664 0000000 0000000 00000011063 13304473505 0030626 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit, OnDestroy, ViewChild, NgZone, ElementRef, AfterViewInit } from '@angular/core';
import { FormBuilder, FormGroup, Validators } from '@angular/forms';
import { Router } from '@angular/router';

import { MapsAPILoader, GoogleMapsAPIWrapper } from '@agm/core';
import { MapService } from '@services/map.service';

import { CustomValidators } from '@validators/custom.validator';
import { Helpers } from '@functions/helpers';

import { SearchField } from '@models/searchField.model';
import { Place } from '@models/place.model';

import { DataService } from '@services/data.service';
import { ToasterService } from '@services/toaster.service';

import * as moment from 'moment';

@Component({
    selector: 'md-search',
    templateUrl: './search.component.html',
    styleUrls: ['./search.component.scss']
})
export class SearchComponent implements OnInit, AfterViewInit, OnDestroy {

    @ViewChild('departure')
    public departureRef: ElementRef;
    @ViewChild('arrival')
    public arrivalRef: ElementRef;

    @ViewChild('map') mapRef;
    map: any;

    toggle = false;

    searchForm: FormGroup;

    now: string;
    currentHour: string;
    currentMinute: string;
    future: string;

    radiusRange: number[];
    radiusMin = 1;
    radiusMax = 30;

    lowPrecision = false;

    radiusOptions = {
        min: this.radiusMin,
        max: this.radiusMax
    };

    places: Place[] = [];
    data: any = {};

    constructor(public mapSvc: MapService,
                private builder: FormBuilder,
                private toasterSvc: ToasterService,
                private dataSvc: DataService,
                private router: Router,
                private toastSvc: ToasterService) {
        this.now = moment().format('YYYY-MM-DD');
        this.currentHour = moment().add(1, 'h').format('hh');
        this.currentMinute = '00';
        this.future = moment().add(1, 'y').format('YYYY-MM-DD');
        this.radiusRange = Helpers.getRange(this.radiusMax);
        this._initForm();
    }

    ngOnInit() {
        this.mapSvc.initField([this.departureRef, this.arrivalRef]);
        this.mapSvc.setCurrentPosition();
        this.mapSvc.initAutocomplete();
        this.mapSvc.listen();

        this.listenValidate();
    }

    ngAfterViewInit() {
        this.mapRef.mapReady.subscribe(map => this.map = map);
    }

    continue() {
        if (this.searchForm.valid) {
            const values = this.searchForm.value;
            this.places[0] = this.mapSvc.fields[0].place;
            this.places[1] = this.mapSvc.fields[1].place;
            this.data = {
                places: this.places,
                passengers: values.passengers,
                datetime: {
                    date: values.datetime.date,
                    hours: values.datetime.hours,
                    minutes: values.datetime.minutes
                },
                radius: values.radius,
                radiusOptions: this.radiusOptions
            };
            this.dataSvc.update(this.data);
            this.router.navigate(['ride', 'results']);
        }
    }

    rangeChange(value: any) {
        this.mapSvc.radius = value;
        this.mapSvc.zoom = Math.round(12 - value / 6);
        this.searchForm.patchValue({
            radius: value
        });
    }


    listenValidate() {
        this.mapSvc.placeChanged.subscribe(
            succcess => this.validate()
        );
    }

    validate() {
        this.searchForm.controls['places'].updateValueAndValidity();
    }

    toggleIcon() {
        this.toggle = !this.toggle;
        this.validate();
        this.departureRef.nativeElement.value = this.toggle ? 'Ma position' : '';
        this.departureRef.nativeElement.disabled = this.toggle;
    }

    ngOnDestroy() {
        this.mapSvc.reset();
    }

    private _initForm() {
        this.searchForm = this.builder.group({
            places: this.builder.group({
                departure: '',
                arrival: '',
            }, { validator: CustomValidators.hasPlaces(this.mapSvc.fields) }),
            radius: [this.radiusMin, [Validators.required, Validators.min(this.radiusMin), Validators.max(this.radiusMax)]],
            passengers: [1, [Validators.required, Validators.min(1)]],
            datetime: this.builder.group({
                date: [this.now, [Validators.required, CustomValidators.notInThePastDateInput, CustomValidators.lessThanOneYear]],
                hours: [this.currentHour, Validators.required],
                minutes: [this.currentMinute, Validators.required]
            }, { validator: CustomValidators.notInThePast })
        });
    }

}
