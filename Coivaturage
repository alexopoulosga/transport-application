                                                                                                                                                                                                                                                                                                                                                                                                                                                                              descartesupem-app-457929343ebe/src/modules/shared/pipes/                                            0000775 0000000 0000000 00000000000 13304473505 0023020 5                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        descartesupem-app-457929343ebe/src/modules/shared/pipes/age.pipe.ts                                 0000664 0000000 0000000 00000001035 13304473505 0025057 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Pipe, PipeTransform } from '@angular/core';

@Pipe({name: 'age'})
export class AgePipe implements PipeTransform {
    transform(birthdate: string): number {
        let date = new Date(Date.parse(birthdate));
        let today = new Date();

        let age = today.getFullYear() - date.getFullYear();

        if(today.getMonth() < date.getMonth()){
            age -= 1;
        }
        else if(today.getMonth() == date.getMonth() && today.getDay() < date.getDay()){
            age -= 1;
        }

        return age;
    }
}
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   descartesupem-app-457929343ebe/src/modules/shared/pipes/clamp-pastille-count.pipe.ts                0000664 0000000 0000000 00000000372 13304473505 0030363 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Pipe, PipeTransform } from '@angular/core';

@Pipe({name: 'clampPastilleCount'})
export class ClampPastilleCount implements PipeTransform {
    transform(count: number): string | number {
        return (count > 99) ? '99+' : count;
    }
}
                                                                                                                                                                                                                                                                      descartesupem-app-457929343ebe/src/modules/shared/pipes/duration-humanized.pipe.ts                  0000664 0000000 0000000 00000000550 13304473505 0030133 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Pipe, PipeTransform } from '@angular/core';

import * as moment from 'moment';

@Pipe({name: 'durationHumanized'})
export class DurationHumanizedPipe implements PipeTransform {
    transform(tempdate: string): string {
        const duration = moment.duration(moment().diff(moment(tempdate)));
        return duration.locale('fr').humanize();
    }
}
                                                                                                                                                        descartesupem-app-457929343ebe/src/modules/shared/services/                                         0000775 0000000 0000000 00000000000 13304473505 0023523 5                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        descartesupem-app-457929343ebe/src/modules/shared/services/auth.service.ts                          0000664 0000000 0000000 00000002273 13304473505 0026477 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Injectable } from '@angular/core';
import { Router } from '@angular/router';
import { JwtHelper } from 'angular2-jwt';

import { StorageService } from '@services/storage.service';

@Injectable()
export class AuthService {

    token: string;

    constructor(private storageSvc: StorageService, private jwtHelper: JwtHelper, private router: Router) {
        this.token = this.storageSvc.has('token') ? this.storageSvc.getItem('token') : null;
    }

    loggedIn(): boolean {
        if (this.storageSvc.has('token')) {
            this.token = this.storageSvc.getItem('token');
        }
        return this.storageSvc.has('token') && !this.jwtHelper.isTokenExpired(this.token);
    }

    logOut(): void {
        if (this.loggedIn()) {
            this.storageSvc.removeItem('token');
            this.router.navigate(['/']);
        }
    }

    tokenExpired(): boolean {
        return this.storageSvc.has('token') && this.jwtHelper.isTokenExpired(this.token);
    }

    tutorialDone(): boolean {
        return this.storageSvc.has('tutorial') && +this.storageSvc.getItem('tutorial') === 1;
    }

    getUserId(): number {
        return this.jwtHelper.decodeToken(this.token).user_id;
    }
}
                                                                                                                                                                                                                                                                                                                                     descartesupem-app-457929343ebe/src/modules/shared/services/data.service.ts                          0000664 0000000 0000000 00000000506 13304473505 0026444 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Injectable } from '@angular/core';
import { BehaviorSubject } from 'rxjs/BehaviorSubject';


@Injectable()
export class DataService {

    private source = new BehaviorSubject<any>('');
    data = this.source.asObservable();

    constructor() { }

    update(values: any) {
        this.source.next(values);
    }
}
                                                                                                                                                                                          descartesupem-app-457929343ebe/src/modules/shared/services/http.service.ts                          0000664 0000000 0000000 00000001404 13304473505 0026510 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Injectable } from '@angular/core';
import { ConnectionBackend, Http, Request, RequestOptions, RequestOptionsArgs, Response } from '@angular/http';
import { Observable } from 'rxjs/Observable';

import 'rxjs/add/operator/catch';

@Injectable()
export class HttpService extends Http {

    constructor(backend: ConnectionBackend, options: RequestOptions) {
        super(backend, options);
    }

    request(url: string | Request, options?: RequestOptionsArgs): Observable<Response> {
        return super.request(url, options)
            .catch(this.catchAuthError(this));
    }

    private catchAuthError(self: HttpService) {
        return (error: Response) => {
            console.log(error);
            return Observable.throw(error);
        };
    }

}
                                                                                                                                                                                                                                                            descartesupem-app-457929343ebe/src/modules/shared/services/map.service.ts                           0000664 0000000 0000000 00000021316 13304473505 0026312 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Injectable, ElementRef, NgZone, EventEmitter, Output } from '@angular/core';

import { MapsAPILoader } from '@agm/core';
import { } from 'googlemaps';

import { DataService } from '@services/data.service';
import { ToasterService } from '@services/toaster.service';

import { SearchField } from '@models/searchField.model';
import { Place } from '@models/place.model';
import { Coords } from '@models/coords.model';

@Injectable()
export class MapService {

    fields: SearchField[] = [];

    placeChanged: EventEmitter<any> = new EventEmitter();
    durationChanged: EventEmitter<any> = new EventEmitter();

    defaultCoords = {
        lat: 83.3523,
        lng: -173.40
    };

    defaultBounds = {
        west: { lat: 48.836374, lng: 2.469747 },
        east: { lat: 48.874482, lng: 2.802873 }
    };

    lat: number;
    lng: number;
    zoom = 12;
    radius = 1;

    lowPrecision = false;

    map: any;

    directionSvc: any;
    directionDisplay: any;
    distanceSvc: any;

    private duration = 0;
    private distance = 0;

    constructor(private mapsAPI: MapsAPILoader,
                private ngZone: NgZone,
                private dataSvc: DataService,
                private toasterSvc: ToasterService) { }

    initField(fields: ElementRef[]) {
        for (const f of fields) {
            this.fields.push({
                elm: f,
                autocomplete: null,
                save: null,
                place: null,
                coords: this.defaultCoords
            });
        }
    }

    getDistance() {
        return this.distance;
    }

    getDuration() {
        return this.duration;
    }

    reset() {
        this.resetDistanceAndDuration();
        this.fields = [];
        this.lowPrecision = false;
        this.zoom = 12;
        this.radius = 3;
        this.lat = 0;
        this.lng = 0;
    }

    resetDistanceAndDuration() {
        this.duration = 0;
        this.distance = 0;
    }

    setMap(map: any) {
        this.map = map;
    }

    initAutocomplete() {
        this.mapsAPI.load().then(() => {

            const bounds = new google.maps.LatLngBounds(
                new google.maps.LatLng(this.defaultBounds.west.lat, this.defaultBounds.west.lng),
                new google.maps.LatLng(this.defaultBounds.east.lat, this.defaultBounds.east.lng)
            );

            this.fields.forEach(field => {
                field.autocomplete = new google.maps.places.Autocomplete(field.elm.nativeElement, {
                    types: ['geocode'],
                    componentRestrictions: {country: 'fr'},
                    bounds: bounds
                });
            });

        });
    }

    listen(drawRide: boolean = false) {
        this.mapsAPI.load().then(() => {
           this.fields.forEach(field => {
               field.autocomplete.addListener('place_changed', () => this._handle(field, drawRide));
           });
        });
    }

    setCurrentPosition() {
        if ('geolocation' in navigator) {
            navigator.geolocation.getCurrentPosition((position: any) => {
                this.lat = position.coords.latitude;
                this.lng = position.coords.longitude;
            });
        }
    }

    private _handle(field: SearchField, drawRide: boolean) {
        this.ngZone.run(() => {
            field.save = field.autocomplete.getPlace();
            if (field.save.geometry === undefined || field.save.geometry === null) {
                return;
            }

            field.place = this._parseGeolocation(
                field.save.address_components,
                field.save.geometry.location.lat(),
                field.save.geometry.location.lng()
            );

            this._setCoords(field);

            this._hasChanged();

            if (drawRide) {
                this.printRide(this.map);
            }

        });
    }

    geolocate(target) {
        if ('geolocation' in navigator) {
            navigator.geolocation.getCurrentPosition(
                postion => this.geolocateSuccess(postion),
                error => this.toasterSvc.error('erreur lors de la gŒì¬©olocalisation'),
                {
                    enableHighAccuracy: true,
                    timeout: 5000,
                    maximumAge: 0
                }
            );
        }
    }

    printRide(map: any) {
        this.ngZone.run(() => {
            for (const field of this.fields) {
                if (field.place === null) {
                    return;
                }
            }
            this.displayRide(map);
        });
    }

    initDisplayRide() {
        this.mapsAPI.load().then(() => {
            this.directionSvc = new google.maps.DirectionsService;
            this.directionDisplay = new google.maps.DirectionsRenderer;
            this.directionDisplay.setMap(this.map);
        });
    }

    displayRide(map: any) {
        this.directionSvc.route({
            origin: new google.maps.LatLng(this.fields[0].coords.lat, this.fields[0].coords.lng),
            destination: new google.maps.LatLng(this.fields[1].coords.lat, this.fields[1].coords.lng),
            optimizeWaypoints: true,
            travelMode: google.maps.TravelMode['DRIVING']
        }, (res, status) => {
            if (status.toString() === 'OK') {
                this.directionDisplay.setMap(map);
                this.directionDisplay.setDirections(res);
            } else {
                this.toasterSvc.error('Erreur lors de l\'affichage du trajet');
            }
        });
    }

    calculateDurationAndDistance(departure: Place, arrival: Place) {
        this.mapsAPI.load().then(() => {
            this.distanceSvc = new google.maps.DistanceMatrixService();
            this.distanceSvc.getDistanceMatrix({
                origins: [new google.maps.LatLng(departure.latitude, departure.longitude)],
                destinations: [new google.maps.LatLng(arrival.latitude, arrival.longitude)],
                travelMode: google.maps.TravelMode['DRIVING']
            }, (res, status) => {
                if (status.toString() === 'OK') {
                    this._getDistAndDuration(res.rows[0].elements[0]);
                    this._durationHasChanged();
                } else {
                    this.toasterSvc.error('impossible de calculer le temps de trajet');
                }
            });
        });
    }

    private _getDistAndDuration(data: any) {
        this.duration = Math.round(data.duration.value / 60);
        this.distance = Math.round(data.distance.value / 100) / 10;
    }

    private _hasChanged() {
        this.placeChanged.emit(null);
    }

    private _durationHasChanged() {
        this.durationChanged.emit(null);
    }

    private _setCoords(field: SearchField) {
        let count = 0;
        this.fields.forEach(f => {
            if (f.place !== null && f.place.lowPrecision) {
                count++;
            }
        });
        this.lowPrecision = count !== 0;

        field.coords = {
            lat: field.save.geometry.location.lat(),
            lng: field.save.geometry.location.lng()
        };

        this.lat = field.coords.lat;
        this.lng = field.coords.lng;
    }

    private geolocateSuccess(position: any) {
        const geocoder = new google.maps.Geocoder;
        const lat = position.coords.latitude;
        const lng = position.coords.longitude;
        const latlng = { lat, lng };

        geocoder.geocode({
            'location': latlng
        }, (res, status) => {
            if (status.toString() === 'OK') {
                this.fields[0].place = this._parseGeolocation(res[0].address_components, lat, lng);
                this._hasChanged();
                if (this.fields[0].place.lowPrecision) {
                    this.lowPrecision = true;
                } elseŒí¬†{
                    this.fields[0].coords = {lat, lng};
                }
            }
        });

        this.lat = lat;
        this.lng = lng;


        this.fields[0].coords.lat = lat;
        this.fields[0].coords.lng = lng;
    }

    private _parseGeolocation(values: any, lat: number, lng: number): Place {
        const place: Place = <Place>{};
        values.map(value => {
            switch (value.types[0]) {
                case 'street_number':
                    place.place = value.long_name;
                    break;
                case 'route':
                    place.street = value.long_name;
                    break;
                case 'locality':
                    place.city = value.long_name;
                    break;
                case 'postal_code':
                    place.postal_code = value.long_name;
                    break;
            }
        });

        place.lowPrecision = !place.postal_code ? true : false;
        place.latitude = lat;
        place.longitude = lng;
        return place;
    }
}
                                                                                                                                                                                                                                                                                                                  descartesupem-app-457929343ebe/src/modules/shared/services/request.service.ts                       0000664 0000000 0000000 00000003651 13304473505 0027227 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Injectable } from '@angular/core';
import { Headers, RequestOptions, URLSearchParams } from '@angular/http';

import { StorageService } from '@services/storage.service';
import { AuthService } from '@services/auth.service';
import { environment } from '@env';

@Injectable()
export class RequestService {

    // prod or dev api location
    private baseUrl: string;

    constructor(private storageSvc: StorageService,
                private authService: AuthService) {
        this.baseUrl = `${environment.api.base}${environment.api.endpoint}`;
    }

    /**
     * API URL to fetch
     * @param { string } resource: string;
     * @returns { string }
     *
     * @memberOf RequestService
     */
    getUrl(resource: string, get: boolean = false): string {
        // return get === false ? `${this.baseUrl}/${resource}/` :  `${this.baseUrl}/${resource}`;
        return `${this.baseUrl}/${resource}`;
    }

    /**
     * Generate requests options
     * @param { bool } isSecure
     * @param { string | null } params
     *
     * @returns { RequestOptions }
     * @memberOf RequestService
     */
    generateOptions(isSecure: boolean = true, object?: Object): RequestOptions {
        const headers = new Headers({'Content-Type': 'application/json'});
        if (isSecure) {
            if (!this.authService.tokenExpired()) {
                headers.set('Authorization', `Bearer ${this.authService.token}`);
            }
        }
        if (object) {
            const parameters = this._getParams(object);
            return new RequestOptions({headers, search: parameters });
        }
        return new RequestOptions({headers});
    }


    private _getParams(object: Object): URLSearchParams {
        const params = new URLSearchParams();

        for (const key in object) {
            if (object.hasOwnProperty(key)) {
                params.set(key, object[key]);
            }
        }
        return params;
    }

}
