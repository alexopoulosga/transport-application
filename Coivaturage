                                                                                                                                                                                                                                                                                                                                                                        descartesupem-app-457929343ebe/src/modules/car/components/list/list.component.ts                    0000664 0000000 0000000 00000003755 13304473505 0027675 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit } from '@angular/core';
import { Router } from '@angular/router';
import { Observable } from 'rxjs/Observable';

import { Car } from '@models/car.model';
import { CarStore } from '@stores/car.store';

import { ToasterService } from '@services/toaster.service';

@Component({
    selector: 'md-list',
    templateUrl: './list.component.html',
    styleUrls: ['./list.component.scss']
})
export class ListComponent implements OnInit {

    cars: Car[];
    results: number;
    selectedCar: Car;

    showLightbox = false;
    loaded = false;
    isEditing = false;

    constructor(private carStore: CarStore, private toasterSvc: ToasterService) { }

    ngOnInit() {
        this.getCars();
    }

    refreshCar(car: Car) {
        this.getCars();
    }

    updateLightbox($event: any) {
        this.closeLightbox();
        this.refreshCar($event);
    }

    closeLightbox() {
        this.showLightbox = false;
    }

    deleteCar(id: number) {
        if (!confirm('Etes-vous certain de vouloir supprimer cette voiture ?')) {
            return;
        }

        this.carStore.deleteCar(id)
            .subscribe(
                success => {
                    console.log('success', success);
                    this.toasterSvc.success('Voiture supprimŒì¬©e');
                    this.getCars();
                    this.closeLightbox();
                },
                error => this.toasterSvc.error('Impossible de supprimer cette voiture car un trajet est planifiŒì¬©')
            );
    }

    getCars() {
        this.carStore.getUserCars()
            .subscribe(
                data => {
                    this.cars = data;
                    this.results = data.length;
                },
                error => console.log('error', error),
                () => this.loaded = true
            );
    }

    openLightbox(car: Car) {
        this.showLightbox = true;
        this.selectedCar = car !== null ? car : {} as Car;
        this.isEditing = car !== null;
    }
}
