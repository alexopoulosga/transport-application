                                                                                                                                                                                                                                                                                                                                                                         descartesupem-app-457929343ebe/src/modules/user/components/preferences/preferences.component.ts     0000664 0000000 0000000 00000004627 13304473505 0032761 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit } from '@angular/core';
import { FormBuilder, FormGroup, Validators } from '@angular/forms';
import { Router } from '@angular/router';

import { UserStore } from '@stores/user.store';

import { Preferences } from '@models/preferences.model';

import { ToasterService } from '@services/toaster.service';

@Component({
    selector: 'md-preferences',
    templateUrl: './preferences.component.html',
    styleUrls: ['./preferences.component.scss']
})
export class PreferencesComponent implements OnInit {

    loaded = false;
    preferences: Preferences[];

    error = false;
    success = false;

    prefForm: FormGroup;

    constructor(private userStore: UserStore,
                private builder: FormBuilder,
                private toasterSvc: ToasterService,
                private router: Router) {
        this._initForm();
    }

    ngOnInit() {
        this.userStore.getAuthUserPreferences()
            .subscribe(
                data => {
                    this.preferences = data;
                    this._setValue();
                },
                error => this.router.navigate(['error', error.status]),
                () => this.loaded = true
            );
    }

    savePref() {
        if (this.prefForm.valid) {
            const pref = this._prepareUpdate(this.prefForm.value);
            this.userStore.updateAuthUserPreferences(pref)
                .subscribe(
                    success => this.toasterSvc.success('PrŒì¬©fŒì¬©rences sauvegardŒì¬©es'),
                    error => this.toasterSvc.error('Erreur lors de la sauvegarde des prŒì¬©fŒì¬©rences')
                );
        } else {
            this.toasterSvc.error('Formulaire invalide');
        }
    }

    private _prepareUpdate(values: any) {
        const pref: Preferences[] = <Preferences[]>[];
        pref.push({ activated: values.smoke, ride_preference_id: 1 });
        pref.push({ activated: values.food, ride_preference_id: 2 });
        pref.push({ activated: values.luggage, ride_preference_id: 3 });
        return pref;
    }

    private _setValue() {
        this.prefForm.setValue({
            smoke: this.preferences[0].activated,
            food: this.preferences[1].activated,
            luggage: this.preferences[2].activated
        });
    }

    private _initForm() {
        this.prefForm = this.builder.group({
            smoke: '',
            food: '',
            luggage: ''
        });
    }

}
