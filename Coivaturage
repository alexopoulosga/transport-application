                                                                                                                                                                                                                                                                                                                                                                                                            descartesupem-app-457929343ebe/src/modules/user/components/detail/detail.component.ts               0000664 0000000 0000000 00000010356 13304473505 0030657 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, Output, OnInit, OnDestroy } from '@angular/core';
import { FormBuilder, FormGroup, Validators } from '@angular/forms';
import { Router } from '@angular/router';
import { Location } from '@angular/common';
import { Subscription } from 'rxjs/Subscription';

import { UserStore } from '@stores/user.store';
import { Subscription as Sub } from '@models/subscription.model';
import { DataService } from '@services/data.service';
import { ToasterService } from '@services/toaster.service';

import { Helpers } from '@functions/helpers';
import { CustomValidators } from '@validators/custom.validator';

import * as moment from 'moment';

@Component({
    selector: 'md-detail',
    templateUrl: './detail.component.html',
    styleUrls: ['./detail.component.scss']
})
export class DetailComponent implements OnInit, OnDestroy {

    me: Sub;

    userForm: FormGroup;
    subscription: Subscription;

    loaded = false;
    success = false;
    mailAlreadyTaken = false;

    currentYear: number;
    maxDate: string;
    dayRange: number[];
    yearRange: number[];

    constructor(private userStore: UserStore,
                private router: Router,
                private builder: FormBuilder,
                private dataSvc: DataService,
                private toasterSvc: ToasterService) {
        this._initForm();

        this.currentYear = moment().year();
        this.maxDate = moment().locale('fr').format('YYYY-MM-DD');
        this.dayRange = Helpers.getRange(31);
        this.yearRange = Helpers.getRange(100);
    }

    ngOnInit() {
        this._getData();
    }

    private _getData() {
        this.subscription = this.dataSvc.data.subscribe(
            data => {
                if (data.name !== undefined) {
                    this.me = data;
                    this._setValue();
                }
            },
            err => console.log('err', err),
        );
    }

    saveUser() {
        if (this.userForm.valid) {
            const user = this._prepareUpdate(this.userForm.value);
            this.userStore.updateAuthUser(user)
                .subscribe(
                    success => {
                        this.toasterSvc.success('Informations sauvegardŒì¬©es');
                    },
                    error => this.toasterSvc.error('Cet email est dŒì¬©jŒì¬† pris')
                );
        }
    }

    private _initForm() {
        this.userForm = this.builder.group({
            name: ['', Validators.required],
            surname: ['', Validators.required],
            phone: ['', [Validators.required, Validators.minLength(10), Validators.maxLength(10)]],
            email: ['', Validators.required],
            passwords: this.builder.group({
                password: ['', Validators.minLength(8)],
                password_confirmation: ['', Validators.minLength(8)]
            }),
            userType: ['', Validators.required],
            birthdate: this.builder.group({
                day: ['', Validators.required],
                month: ['', Validators.required],
                year: ['', [Validators.required, CustomValidators.yearValidator]]
            })
        });
    }

    private _setValue() {
        const birth = this.me.birthdate.split(' ')[0].split('-');
        this.userForm.setValue({
            name: this.me.name,
            surname: this.me.surname,
            email: this.me.email,
            phone: this.me.phone,
            passwords: {
                password: '',
                password_confirmation: '',
            },
            userType: this.me.user_type_id,
            birthdate: {
                day: +birth.pop(),
                month: +birth.pop(),
                year: +birth.pop()
            }
        });
    }

    private _prepareUpdate(values: any): Sub {
        const user: Sub = <Sub>{};
        user.name = values.name;
        user.email = values.email;
        user.surname = values.surname;
        user.password = values.password;
        user.password_confirmation = values.confirmation;
        user.phone = values.phone;
        user.birthdate = `${values.birthdate.year}-${values.birthdate.month}-${values.birthdate.day} 00:00:00`;
        user.user_type_id = values.userType;
        return user;
    }

    ngOnDestroy() {
        this.subscription.unsubscribe();
    }
}
