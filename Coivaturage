                                                                                                                                                                                                                                                                                                                                                                                                                                                                 descartesupem-app-457929343ebe/src/modules/shared/functions/                                        0000775 0000000 0000000 00000000000 13304473505 0023710 5                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        descartesupem-app-457929343ebe/src/modules/shared/functions/helpers.ts                              0000664 0000000 0000000 00000001143 13304473505 0025721 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { ShortBio } from '@models/shortbio.model';
import { Ride } from '@models/ride.model';

export class Helpers {
    static getRandomInt(min: number, max: number) {
        return Math.floor(Math.random() * (max - min + 1)) + min;
    }

    static getRange(size: number) {
        return Array(size).fill(0, 0, size).map((x, i) => i + 1);
    }

    static exists(value: any) {
        return value !== undefined && value !== null;
    }

    static empty(array: any[]) {
        return !this.exists(array[0]);
    }

    static log3(x: number): number {
        return Math.log(x) / Math.log(3);
    }
}
                                                                                                                                                                                                                                                                                                                                                                                                                             descartesupem-app-457929343ebe/src/modules/shared/guards/                                           0000775 0000000 0000000 00000000000 13304473505 0023165 5                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        descartesupem-app-457929343ebe/src/modules/shared/guards/auth.guard.ts                              0000664 0000000 0000000 00000001137 13304473505 0025601 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Injectable } from '@angular/core';
import { Router, CanActivate } from '@angular/router';

import { Observable } from 'rxjs/Observable';
import 'rxjs/add/observable/of';

import { AuthService } from '@shared/services/auth.service';

@Injectable()
export class AuthGuard implements CanActivate {

    constructor (private router: Router, private authSvc: AuthService) { }

    canActivate(): Observable<boolean> {
        if (this.authSvc.loggedIn()) {
            return Observable.of(true);
        }

        this.router.navigate(['/error', '403']);
        return Observable.of(false);
    }

}
                                                                                                                                                                                                                                                                                                                                                                                                                                 descartesupem-app-457929343ebe/src/modules/shared/guards/logged.guard.ts                            0000664 0000000 0000000 00000001133 13304473505 0026075 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Injectable } from '@angular/core';
import { Router, CanActivate } from '@angular/router';

import { Observable } from 'rxjs/Observable';
import 'rxjs/add/observable/of';

import { AuthService } from '@shared/services/auth.service';

@Injectable()
export class LoggedGuard implements CanActivate {

    constructor(private router: Router, private authSvc: AuthService) { }

    canActivate(): Observable<boolean> {
        if (this.authSvc.loggedIn()) {
            this.router.navigate(['/ride']);
            return Observable.of(false);
        }
        return Observable.of(true);
    }

}
                                                                                                                                                                                                                                                                                                                                                                                                                                     descartesupem-app-457929343ebe/src/modules/shared/models/                                           0000775 0000000 0000000 00000000000 13304473505 0023163 5                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        descartesupem-app-457929343ebe/src/modules/shared/models/bookIn.model.ts                            0000664 0000000 0000000 00000000110 13304473505 0026043 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        export interface BookIn {
