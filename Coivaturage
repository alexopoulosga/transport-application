                                                                                                                                                                                                                                                                                                                                                                         descartesupem-app-457929343ebe/src/modules/auth/components/newPassword/newPassword.component.ts     0000664 0000000 0000000 00000004250 13304473505 0032762 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit } from '@angular/core';
import { ActivatedRoute, Router } from '@angular/router';
import { FormBuilder, FormGroup, Validators } from '@angular/forms';
import { Observable } from 'rxjs/Observable';

import { CustomValidators } from '@validators/custom.validator';
import { AuthStore } from '@stores/auth.store';
import { ToasterService } from '@services/toaster.service';

@Component({
  selector: 'md-new-password',
  templateUrl: './newPassword.component.html',
  styleUrls: ['./newPassword.component.scss']
})
export class NewPasswordComponent implements OnInit {

    passwordForm: FormGroup;
    token: string;

    passwords = {
        password: '',
        password_confirmation: '',
        token: ''
    };

    constructor(private builder: FormBuilder,
                private route: ActivatedRoute,
                private router: Router,
                private authStore: AuthStore,
                private toasterSvc: ToasterService) {
        this._initForm();
    }

    ngOnInit() {
        const token: Observable<string> = this.route.params.map(param => param.token);
        token.subscribe((parameter: string) => {
            this.token = parameter;
        });
    }

    resetPassword() {
        if (this.passwordForm.valid) {
            this._prepareSave(this.passwordForm.value);
            this.authStore.resetPassword(this.passwords)
                .subscribe(
                    success => {
                        this.toasterSvc.success('Mot de passe modifŒì¬©');
                        this.router.navigate(['/auth/signin']);
                    },
                    error => this.toasterSvc.error('Erreur lors du changement du mot de passe')
                );
        }
    }

    private _initForm() {
        this.passwordForm = this.builder.group({
            password: ['', Validators.required],
            password_confirmation: ['', Validators.required]
        }, {validator: CustomValidators.confirmPassword});
    }

    private _prepareSave(values: any) {
        this.passwords.password = values.password;
        this.passwords.password_confirmation = values.password_confirmation;
        this.passwords.token = this.token;
    }

}
