                                                                                                                                                                                                                                                                                                                                                                                                                          descartesupem-app-457929343ebe/src/modules/comment/components/list/list.component.ts                0000664 0000000 0000000 00000002116 13304473505 0030560 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit, Input } from '@angular/core';

import { CommentStore } from '@stores/comment.store';

import { Comment } from '@models/comment.model';

import localeFr from '@angular/common/locales/fr';
import { registerLocaleData } from '@angular/common';

@Component({
    selector: 'md-comment',
    templateUrl: './list.component.html',
    styleUrls: ['./list.component.scss']
})
export class ListComponent implements OnInit {

    comments: Comment[];

    @Input() userId;

    constructor(private commentStore: CommentStore) {
        registerLocaleData(localeFr);
    }

    ngOnInit() {
        this.userId === undefined ? this.getAuthUserComments() : this.getUserComments(this.userId);
    }

    getAuthUserComments() {
        this.commentStore.getAuthUserComments().subscribe(
            data => this.comments = data,
            err => console.log('err', err),
        );
    }

    getUserComments(id: number) {
        this.commentStore.getUserComment(id).subscribe(
            data => this.comments = data,
            err => console.log('err', err)
        );
    }

}
