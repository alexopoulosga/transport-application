                                                                                                                                                                             descartesupem-app-457929343ebe/src/modules/app/app.routing.ts                                       0000664 0000000 0000000 00000003004 13304473505 0024025 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { RouterModule } from '@angular/router';

import { ErrorComponent } from '@app/components/error/error.component';
import { TcuComponent } from './components/tcu/tcu.component';

import { AuthGuard } from '@guards/auth.guard';

const routes = [
    {
        path: '',
        loadChildren: 'modules/auth/auth.module#AuthModule',
    },
    {
        path: 'user',
        redirectTo: 'user/detail',
        pathMatch: 'full'
    },
    {
        path: 'ride',
        redirectTo: 'ride/search',
        pathMatch: 'full'
    },
    {
        path: 'notification',
        loadChildren: 'modules/notification/notification.module#NotificationModule',
        canActivate: [AuthGuard]
    },
    {
        path: 'user',
        loadChildren: 'modules/user/user.module#UserModule',
        canActivate: [AuthGuard]
    },
    {
        path: 'ride',
        loadChildren: 'modules/ride/ride.module#RideModule',
        canActivate: [AuthGuard]
    },
    {
        path: 'cgu',
        component: TcuComponent,
        canActivate: [AuthGuard]
    },
    {
        path: 'reservation',
        loadChildren: 'modules/reservation/reservation.module#ReservationModule',
        canActivate: [AuthGuard]
    },
    {
        path: 'chat',
        loadChildren: 'modules/chat/chat.module#ChatModule',
        canActivate: [AuthGuard]
    },
    {
        path: 'error/:code',
        component: ErrorComponent
    },
    {
        path: '**',
        component: ErrorComponent
    },
];

export const Routing = RouterModule.forRoot(routes);
