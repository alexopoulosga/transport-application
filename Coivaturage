                                                                                                                                                                                                                                                                                                                                                                                                                          descartesupem-app-457929343ebe/src/modules/car/components/edit/edit.component.ts                    0000664 0000000 0000000 00000005556 13304473505 0027622 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, Input, Output, OnChanges, EventEmitter } from '@angular/core';
import { FormBuilder, FormGroup, Validators } from '@angular/forms';
import { Response } from '@angular/http';

import { Car } from '@models/car.model';
import { CarStore } from '@stores/car.store';

import { ToasterService } from '@services/toaster.service';

@Component({
    selector: 'md-edit',
    templateUrl: './edit.component.html',
    styleUrls: ['./edit.component.scss']
})
export class EditComponent implements OnChanges {

    @Input() car: Car;
    @Input() isEditing: boolean;
    @Output() carEvent: EventEmitter<boolean> = new EventEmitter();

    id: number;
    carForm: FormGroup;

    success = false;
    error = false;

    constructor(private builder: FormBuilder,
                private carStore: CarStore,
                private toaster: ToasterService) {
        this._initForm();
    }

    ngOnChanges() {
        if (this.car.id) {
            this.id = +this.car.id;
            this.isEditing = true;
        }
        this._setForm();
    }

    saveCar() {
        if (this.carForm.valid) {
            const car: Car = this._prepareUpdate(this.carForm.value);
            if (this.isEditing) {
                this._edit(this.id, car);
            } else {
                this._create(car);
            }
        }
    }

    private _edit(id: number, car: Car) {
        this.carStore.udpateCar(id, car)
            .subscribe(
                data => {
                    this.toaster.success('Changements sauvegardŒì¬©s');
                    this.carEvent.emit(true);
                },
                error => this.toaster.error('Impossible d\'effectuer les changements')
            );
    }

    private _create(car: Car) {
        this.carStore.createCar(car)
            .subscribe(
                success => {
                    this.toaster.success('Voiture ajoutŒì¬©e');
                    this.carForm.reset();
                    this.carEvent.emit(true);
                },
                error => this.toaster.error('Impossible d\'ajouter cette voiture')
        );
    }

    private _setForm() {
        this.carForm.setValue({
            brand: this.car.brand ? this.car.brand : '',
            model: this.car.model ? this.car.model : '',
            color: this.car.color ? this.car.color : '',
            place: this.car.place ? this.car.place : ''
        });
    }

    private _initForm() {
        this.carForm = this.builder.group({
            brand: ['', Validators.required],
            model: ['', Validators.required],
            color: ['', Validators.required],
            place: ['', Validators.min(1)]
        });
    }

    private _prepareUpdate(values: any) {
        const car: Car = <Car>{};
        car.brand = values.brand;
        car.model = values.model;
        car.color = values.color;
        car.place = values.place;
        return car;
    }

}
