                                                                                                                                                                                                                                                                                                                                                                                                            descartesupem-app-457929343ebe/src/modules/reservation/components/filter/filter.component.ts        0000664 0000000 0000000 00000002350 13304473505 0032303 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit, Output, OnChanges, EventEmitter } from '@angular/core';
import { FormGroup, FormBuilder, FormControl, Validators } from '@angular/forms';

import { DataService } from '@services/data.service';

@Component({
    selector: 'md-filter',
    templateUrl: './filter.component.html',
    styleUrls: ['./filter.component.scss']
})
export class FilterComponent implements OnInit {

    @Output() closeEvent: EventEmitter<boolean> = new EventEmitter();

    filterForm: FormGroup;

    constructor(private builder: FormBuilder, private dataSvc: DataService) {
        this._initForm();
        this.dataSvc.update(this.filterForm.value);
    }

    ngOnInit() {
        this.listenChanges();
    }

    resetForm() {
        this.filterForm.reset();
        this._initForm();
    }

    listenChanges() {
        this.filterForm.valueChanges.subscribe(values => {
            this.dataSvc.update(values);
        });
    }

    closeLightbox() {
        this.closeEvent.emit(true);
    }

    private _initForm() {
        this.filterForm = this.builder.group({
            filter: ['status', Validators.required],
            order: ['asc', Validators.required],
            status: ['coming', Validators.required]
        });
    }

}
