export function getDriverBio(ride: Ride) {
    const obj: ShortBio = {
        name: ride.d_name,
        surname: ride.d_surname,
        type: ride.d_label,
        birthdate: ride.d_birthdate,
        rating: ride.d_rating,
        rating_count: ride.d_rating_count,
        avatar: ride.d_avatar
    };

    return obj;
}                                                                                                                                  descartesupem-app-457929343ebe/src/modules/user/user.constant.ts                                    0000664 0000000 0000000 00000001035 13304473505 0024565 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { HomeComponent } from '@user/components/home/home.component';
