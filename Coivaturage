                                                                                                                                                                                                                                                                                                                                                                                                                          descartesupem-app-457929343ebe/src/modules/ride/components/post/post.component.ts                   0000664 0000000 0000000 00000013636 13304473505 0030076 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit, OnDestroy, AfterViewInit, ViewChild, NgZone, ElementRef } from '@angular/core';
import { FormBuilder, FormGroup, Validators } from '@angular/forms';
import { Router } from '@angular/router';

import { DataService } from '@services/data.service';
import { MapService } from '@services/map.service';

import { Place } from '@models/place.model';
import { Coords } from '@models/coords.model';
import { Car } from '@models/car.model';

import { Helpers } from '@functions/helpers';

import { CarStore } from '@stores/car.store';
import { CustomValidators } from '@validators/custom.validator';

import * as moment from 'moment';

@Component({
    selector: 'md-post',
    templateUrl: './post.component.html',
    styleUrls: ['./post.component.scss']
})
export class PostComponent implements OnInit, AfterViewInit, OnDestroy {

    lat: number;
    lng: number;
    zoom: number;

    tomorrow: string;
    currentHour: string;
    future: any;

    cars: Car[];
    carSelected: Car;

    lowPrecision = false;
    error = false;
    loaded = false;
    haveCars = true;

    toggle = false;

    data = {};

    countryRestrict = {
        country: 'fr'
    };
    defaultBounds = {
        west: { lat: 48.836374, lng: 2.469747 },
        east: { lat: 48.874482, lng: 2.802873 }
    };
    defaultCoords = {
        lat: 83.3523, lng: -173.40
    };

    places: Place[] = [];

    rideForm: FormGroup;

    departure: any;
    arrival: any;

    @ViewChild('departure')
    public departureRef: ElementRef;
    @ViewChild('arrival')
    public arrivalRef: ElementRef;

    @ViewChild('map') mapRef;
    map: any;

    constructor(private builder: FormBuilder,
                public mapSvc: MapService,
                private dataSvc: DataService,
                private carStore: CarStore,
                private router: Router) {
            this.tomorrow = moment().add(1, 'd').format('YYYY-MM-DD');
            this.currentHour = moment().format('hh');
            this.future = moment().add(1, 'y').format('YYYY-MM-DD');
            this._initForm();
    }

    ngOnInit() {
        this.mapSvc.initField([this.departureRef, this.arrivalRef]);
        this.mapSvc.setCurrentPosition();
        this.mapSvc.initAutocomplete();

        this.listenValidate();
        this.getUserCar();
    }

    ngAfterViewInit() {
        this.mapRef.mapReady.subscribe(map => {
            this.map = map;
            this.mapSvc.setMap(this.map);
        });
        this.mapSvc.initDisplayRide();
        this.mapSvc.listen(true);
    }

    listenValidate() {
        this.mapSvc.placeChanged.subscribe(
            success => this.validate()
        );
    }

    toggleIcon() {
        this.toggle = !this.toggle;
        this.departureRef.nativeElement.value = this.toggle ? 'Ma position' : '';
        this.departureRef.nativeElement.disabled = this.toggle;
    }

    getUserCar() {
        this.carStore.getUserCars().subscribe(
            data => this.cars = data,
            error => this.router.navigate(['error', error.status]),
            () => {
                this.loaded = true;
                this.haveCars = this.cars.length !== 0;
                if (this.haveCars) {
                    this.carSelected = this.cars[0];
                    this._setValue();
                }
            }
        );
    }

    onSelectCar() {
        const carId = +this.rideForm.value.car.id;
        this.carSelected = this.cars.find((car: Car) => car.id === carId);
        this.rideForm.get('passengers').setValidators([
            Validators.required,
            Validators.min(1),
            Validators.max(this.carSelected.place)
        ]);
        this._setValue();
    }

    continue() {
        if (this.rideForm.valid) {
            this.places[0] = this.mapSvc.fields[0].place;
            this.places[1] = this.mapSvc.fields[1].place;
            this.data = {
                places: this.places,
                car: {
                    id: this.carSelected.id,
                    brand: this.carSelected.brand,
                    model: this.carSelected.model
                },
                passengers: this.rideForm.value.passengers,
                datetime: {
                    date: this.rideForm.value.datetime.date,
                    hours: this.rideForm.value.datetime.hours,
                    minutes: this.rideForm.value.datetime.minutes
                },
                details: this.rideForm.value.details
            };
            this.dataSvc.update(this.data);
            this.router.navigate(['/ride', 'confirm']);
        }
    }

    validate() {
        this.rideForm.controls['places'].updateValueAndValidity();
    }


    private _setValue() {
        this.rideForm.patchValue({
            car: {
                id: this.carSelected.id,
                brand: this.carSelected.brand,
                model: this.carSelected.model
            },
            passengers: this.carSelected.place
        });
    }

    private _initForm() {
        this.rideForm = this.builder.group({
            places: this.builder.group({
                departure: '',
                arrival: ''
            }, { validator: CustomValidators.hasPlacesPost(this.mapSvc.fields)}),
            car: this.builder.group({
                id: ['', Validators.required],
                brand: ['', Validators.required],
                model: ['', Validators.required]
            }),
            passengers: ['', [Validators.required, Validators.min(1)]],
            datetime: this.builder.group({
                date: [this.tomorrow, [Validators.required, CustomValidators.notInThePastDateInput, CustomValidators.lessThanOneYear]],
                hours: [this.currentHour, Validators.required],
                minutes: ['00', Validators.required]
            }, {validator: CustomValidators.notInThePast }),
            details: ['', [Validators.minLength(0), Validators.maxLength(150)]]
        });
    }

    ngOnDestroy() {
        this.mapSvc.reset();
    }

}
