                                                                                                                                                                                                                                                                                                                                                                                       descartesupem-app-457929343ebe/src/modules/reservation/components/passenger/passenger.component.ts  0000664 0000000 0000000 00000007127 13304473505 0033516 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit, OnDestroy } from '@angular/core';
import { registerLocaleData } from '@angular/common';
import localeFr from '@angular/common/locales/fr';
import { Subscription } from 'rxjs/Subscription';

import { ReservationStore } from '@stores/reservation.store';

import { Ride } from '@models/ride.model';

import { DataService } from '@services/data.service';
import { ToasterService } from '@services/toaster.service';

@Component({
    selector: 'md-passenger',
    templateUrl: './passenger.component.html',
    styleUrls: ['./passenger.component.scss']
})
export class PassengerComponent implements OnInit {

    rides: Ride[];
    temp: Ride[];

    subscription: Subscription;

    formValues: any;

    loaded = false;

    constructor(private reservationStore: ReservationStore,
                private dataSvc: DataService,
                private toasterSvc: ToasterService) {
        registerLocaleData(localeFr);
    }

    ngOnInit() {
        this.getData();
    }

    cancel(rideId: number) {
        if (!confirm('Voulez vous annulez cette rŒì¬©servation ?')) {
            return;
        }

        this.reservationStore.cancelRide(rideId).subscribe(
            sucess => {
                this.toasterSvc.success('Trajet annulŒì¬©');
                this.getData();
            },
            error => this.toasterSvc.error('Erreur lors de l\'annulation')
        );
    }

    listenFormValues() {
        this.subscription = this.dataSvc.data.subscribe(
            data => {
                this.formValues = data;
                this.filter(this.formValues.status);
            }
        );
    }

    getData() {
        this.reservationStore.getReservationsPassenger().subscribe(
            data => this.rides = this.temp = data,
            err => console.log('err', err),
            () => {
                this.loaded = true;
                this.listenFormValues();
            }
        );
    }

    /* -------------- FILTERS ---------------- */

    sort(key: string, order: string, array: Ride[]) {
        if (this.formValues.filter === 'status') {
            this.rides = array.sort(this.compareValues('a_date', order));
        } else {
            this.rides = array.sort(this.compareValues(key, order));
        }
    }

    filter(key: string) {
        switch (key) {
            case 'all':
                this.sort('a_date', 'asc', this.temp);
                return;
            case 'done':
                this.rides = this.temp.filter(ride => Boolean(ride.canceled_reservation) === false && ride.ride_status === 'done');
                break;
            case 'canceled':
                this.rides = this.temp.filter(ride => Boolean(ride.canceled_reservation) === true || ride.ride_status === 'canceled');
                break;
            default:
                this.rides = this.temp.filter(ride => Boolean(ride.canceled_reservation) === false && ride.ride_status === 'coming');
                break;
        }
        this.sort(this.formValues.filter, this.formValues.order, this.rides);
    }

    compareValues(key: string, order: string) {
        return (a, b) => {

            if (!a.hasOwnProperty(key) || !b.hasOwnProperty(key)) {
                return 0;
            }

            const A = (typeof a[key] === 'string') ? a[key].toUpperCase() : a[key];
            const B = (typeof b[key] === 'string') ? b[key].toUpperCase() : b[key];

            let compare = 0;
            if (A > B) {
                compare = 1;
            } else if (A < B) {
                compare = -1;
            }

            return (order === 'desc') ? (compare * -1) : compare;
        };
    }
}
