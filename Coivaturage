                              descartesupem-app-457929343ebe/src/modules/shared/stores/                                           0000775 0000000 0000000 00000000000 13304473505 0023217 5                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        descartesupem-app-457929343ebe/src/modules/shared/stores/auth.store.ts                              0000664 0000000 0000000 00000004300 13304473505 0025660 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Injectable } from '@angular/core';
import { Response } from '@angular/http';

import { Observable } from 'rxjs/Observable';
import 'rxjs/add/operator/map';
import 'rxjs/add/observable/throw';

import { HttpService } from '@services/http.service';
import { RequestService } from '@services/request.service';
import { StorageService } from '@services/storage.service';

import { Credentials } from '@models/credentials.model';
import { UserToken } from '@models/userToken.model';
import { Subscription } from '@models/subscription.model';

@Injectable()
export class AuthStore {

    constructor(private http: HttpService,
                private requestSvc: RequestService,
                private storageSvc: StorageService) { }

    authentificateUser(credentials: Credentials): Observable<UserToken> {
        return this.http
            .post(this.requestSvc.getUrl('auth/signin'), {'email': credentials.email, 'password': credentials.password}, this.requestSvc.generateOptions(false))
            .map((res: Response) => this.saveToken(res))
            .catch((error: any) => Observable.throw(error.json()));
    }

    subscribeUser(subscription: Subscription): Observable<Subscription> {
        return this.http
            .post(this.requestSvc.getUrl('auth/signup'), subscription, this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((error: any) => Observable.throw(error));
    }

    askForNewPassword(email: string): Observable<string> {
        return this.http
            .post(this.requestSvc.getUrl('users/reset'), email, this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((error: any) => Observable.throw(error));
    }

    resetPassword(passwords: Object): Observable<string> {
        return this.http
            .patch(this.requestSvc.getUrl('users/reseted'), passwords, this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((error: any) => Observable.throw(error));
    }

    private saveToken(res: Response): UserToken {
        const token = res.json() as UserToken;
        this.storageSvc.setItem('token', token.token);
        return token;
    }

}
