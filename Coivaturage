        return this.http
            .get(this.requestSvc.getUrl('users/me/rides/subscribed'), this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    cancelRide(rideId: number): Observable<Ride> {
        return this.http
            .patch(this.requestSvc.getUrl(`users/rides/${rideId}/cancel`), {canceled: true}, this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

}
                                                                                                                                                                                                                                                                                           descartesupem-app-457929343ebe/src/modules/shared/stores/ride.store.ts                              0000664 0000000 0000000 00000003051 13304473505 0025644 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Injectable } from '@angular/core';
import { Response } from '@angular/http';

import { Observable } from 'rxjs/Observable';
import 'rxjs/add/operator/map';
import 'rxjs/add/observable/throw';

import { Ride } from '@models/ride.model';
import { RidePost } from '@models/ridePost.model';
import { RideSearch } from '@models/rideSearch.model';

import { HttpService } from '@services/http.service';
import { RequestService } from '@services/request.service';
import { DataService } from '@services/data.service';

@Injectable()
export class RideStore {

    constructor(private http: HttpService,
                private requestSvc: RequestService,
                private dataSvc: DataService) { }

    getRide(id: number): Observable<Ride> {
        return this.http
            .get(this.requestSvc.getUrl(`rides/${id}`), this.requestSvc.generateOptions())
            .map((res: Response) => res.json() as Ride)
            .catch((err: any) => Observable.throw(err));
        }

    createRide(ride: RidePost): Observable<RidePost> {
        return this.http
            .put(this.requestSvc.getUrl('rides'), ride, this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    searchRides(ride: RideSearch): Observable<Ride[]> {
        return this.http
            .get(this.requestSvc.getUrl('rides/search', true), this.requestSvc.generateOptions(true, ride))
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

}
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       descartesupem-app-457929343ebe/src/modules/shared/stores/user.store.ts                              0000664 0000000 0000000 00000004661 13304473505 0025707 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Injectable } from '@angular/core';
import { Response } from '@angular/http';

import { Observable } from 'rxjs/Observable';
import 'rxjs/add/operator/map';
import 'rxjs/add/observable/throw';

import { HttpService } from '@services/http.service';
import { RequestService } from '@services/request.service';
import { StorageService } from '@services/storage.service';

import { Profile } from '@models/profile.model';
import { Subscription } from '@models/subscription.model';
import { Preferences } from '@models/preferences.model';

@Injectable()
export class UserStore {

    constructor(private http: HttpService,
                private requestSvc: RequestService,
                private storageSvc: StorageService) { }

    getAuthUser(): Observable<Subscription>  {
        return this.http
            .get(this.requestSvc.getUrl('users/me'), this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    updateAuthUser(user: Subscription): Observable<Subscription> {
        return this.http
            .patch(this.requestSvc.getUrl('users/me'), user, this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    setTutorialState(finished: number = 1): Observable<number> {
        return this.http
            .patch(this.requestSvc.getUrl('users/tutorial'), {finished}, this.requestSvc.generateOptions())
            .map((res: Response) => this._saveTutorial())
            .catch((err: any) => Observable.throw(err));
    }

    /* ------------------ PREFERENCES ------------------------- */

    getAuthUserPreferences(): Observable<Preferences[]> {
        return this.http
            .get(this.requestSvc.getUrl('users/me/preferences'), this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    updateAuthUserPreferences(preferences: Preferences[]): Observable<Preferences[]> {
        return this.http
            .patch(this.requestSvc.getUrl('users/me/preferences'), preferences, this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    /* -------------------------------------------------- */

    private _saveTutorial() {
        this.storageSvc.setItem('tutorial', '1');
    }

}

                                                                               descartesupem-app-457929343ebe/src/modules/shared/validators/                                       0000775 0000000 0000000 00000000000 13304473505 0024050 5                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        descartesupem-app-457929343ebe/src/modules/shared/validators/custom.validator.ts                    0000664 0000000 0000000 00000003642 13304473505 0027723 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { FormControl, FormGroup, Validators, AbstractControl } from '@angular/forms';

import { SearchField } from '@models/searchField.model';

import * as moment from 'moment';

export class CustomValidators implements Validators {

    static yearValidator(control: FormControl) {
        const now = moment().year();
        return control.value < now ? null :  { error : true };
    }

    static confirmPassword(control: FormControl) {
        return control.get('password').value === control.get('password_confirmation').value
            ? null : { error : true };
    }

    static notInThePastDateInput(control: FormControl) {
        const now = moment().format('YYYY-MM-DD');
        return control.value >= now ? null : { error: true};
    }

    static notInThePast(control: FormControl) {
        const now = moment().format('YYYY-MM-DD');
        const nowTime = moment().format('HH:mm');
        const time = `${control.value.hours}:${control.value.minutes}`;
        return control.value.date > now || ( control.value.date === now && time > nowTime )
            ? null : { error: true };
    }

    static lessThanOneYear(control: FormControl) {
        const future = moment().add(1, 'y').format('YYYY-MM-DD');
        return control.value <= future ? null : { error: true };
    }

    static hasPlacesPost = (fields: SearchField[]) => {
        return () => {
            for (const field of fields) {
                if (field.place === null || field.place === undefined || field.place.lowPrecision) {
                    return { error: true};
                }
            }
            return null;
        };
    }

    static hasPlaces = (fields: SearchField[]) => {
        return () => {
            for (const field of fields) {
                if (field.place === null || field.place === undefined) {
                    return { error: true };
                }
            }
            return null;
        };
    }

}
