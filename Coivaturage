import { Response } from '@angular/http';

import { Observable } from 'rxjs/Observable';
import 'rxjs/add/operator/map';
import 'rxjs/add/observable/throw';
import 'rxjs/add/observable/interval';

import { Chat } from '@models/chat.model';
import { MessageSend } from '@models/messageSend.model';

import { HttpService } from '@services/http.service';
import { RequestService } from '@services/request.service';

@Injectable()
export class ChatStore {

    seconds = 30;
    interval = this.seconds * 1000;

    constructor(private http: HttpService,
                private requestSvc: RequestService) { }

    getAuthUserChat(): Observable<Chat[]> {
        return this.http
            .get(this.requestSvc.getUrl('chats'), this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    getChat(id: number): Observable<Chat> {
        return this.http
            .get(this.requestSvc.getUrl(`chats/${id}`), this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    sendMessage(message: MessageSend): Observable<MessageSend> {
        return this.http
            .post(this.requestSvc.getUrl('messages'), message, this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    createChat(driverId: number): Observable<number> {
        return this.http
            .post(this.requestSvc.getUrl('chats'), {user_id: driverId}, this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    chatExists(userId: number): Observable<any> {
        return this.http
            .get(this.requestSvc.getUrl(`chats/${userId}/exists`), this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    getCount(): Observable<number> {
        return Observable.interval(this.interval).flatMap(() =>
            this.http
                .get(this.requestSvc.getUrl('chats/unread'), this.requestSvc.generateOptions())
                .map((res: Response) => res.json())
                .catch((err: any) => Observable.throw(err))
        );
    }

}
