describe('mobilite-descartes App', () => {
    let page: AppPage;

    beforeEach(() => {
        page = new AppPage();
    });

    it('should display welcome message', () => {
        page.navigateTo();
        expect(page.getParagraphText()).toEqual('Welcome to app!');
    });
});
                                                                                                                                                                                              descartesupem-app-457929343ebe/e2e/app.po.ts                                                        0000664 0000000 0000000 00000000340 13304473505 0020410 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { browser, by, element } from 'protractor';

export class AppPage {
    navigateTo() {
        return browser.get('/');
    }

    getParagraphText() {
        return element(by.css('app-root h1')).getText();
    }
}
