                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          descartesupem-app-457929343ebe/src/modules/auth/components/subscribe/subscribe.component.ts         0000664 0000000 0000000 00000006730 13304473505 0032101 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit } from '@angular/core';
import { Router } from '@angular/router';
import { FormBuilder, FormGroup, Validators } from '@angular/forms';

import { AuthStore } from '@stores/auth.store';

import { Subscription } from '@models/subscription.model';
import { ToasterService } from '@services/toaster.service';

import * as moment from 'moment';

import { Helpers } from '@functions/helpers';
import { CustomValidators } from '@validators/custom.validator';

@Component({
    selector: 'md-subscribe',
    templateUrl: './subscribe.component.html',
    styleUrls: ['./subscribe.component.scss']
})
export class SubscribeComponent {

    subscribeForm: FormGroup;
    sub: Subscription = <Subscription>{};
    maxDate: string;
    success = false;

    mailAlreadyTaken = false;

    dayRange: number[];
    yearRange: number[];

    currentYear: number;

    constructor(private builder: FormBuilder, private authStore: AuthStore, private toastSvc: ToasterService, private router: Router) {
        this._initForm();

        this.currentYear = moment().year();

        this.maxDate = moment().locale('fr').format('YYYY-MM-DD');
        this.dayRange = Helpers.getRange(31);
        this.yearRange = Helpers.getRange(100);
    }

    subscribe() {
        if (this.subscribeForm.valid) {
            this._prepareSubscription(this.subscribeForm.value);
            this.authStore.subscribeUser(this.sub)
                .subscribe(
                    success => {
                        this.toastSvc.success('Vous ŒìÔ£πtes dŒì¬©sormais inscrit');
                        this.router.navigate(['/auth/']);
                    },
                    error => {
                        if (error.status === 403) {
                            this.toastSvc.error('Cet email est dŒì¬©jŒì¬† pris');
                        } else {
                            this.toastSvc.error('Erreur lors de l\'inscription');
                        }
                    }
                );
        }
    }

    private _initForm() {
        this.subscribeForm = this.builder.group({
            name : ['', Validators.required],
            surname : ['', Validators.required],
            email: ['', [Validators.required, Validators.email]],
            passwords: this.builder.group({
                password: ['', [Validators.required, Validators.minLength(8)]],
                password_confirmation: ['', [Validators.required, Validators.minLength(8)]],
            }, {validator: CustomValidators.confirmPassword}),
            phone : ['', [Validators.required, Validators.minLength(10), Validators.maxLength(10)]],
            userType: [1, Validators.required],
            birthdate: this.builder.group({
                day: [1, Validators.required],
                month: [1, Validators.required],
                year: [1990, [Validators.required, CustomValidators.yearValidator]]
            })
        });
    }

    private _prepareSubscription(values: any) {
        this.sub.name = values.name;
        this.sub.email = values.email;
        this.sub.surname = values.surname;
        this.sub.password = values.passwords.password;
        this.sub.password_confirmation = values.passwords.password_confirmation;
        this.sub.phone = values.phone;
        this.sub.birthdate = `${values.birthdate.year}-${values.birthdate.month}-${values.birthdate.day}`;
        this.sub.birthdate = `${moment(this.sub.birthdate, 'YYYY-MM-DD').format('YYYY-MM-DD')} 00:00:00`;
        this.sub.user_type_id = values.userType;
    }

}
