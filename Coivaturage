import { Ride } from '@models/ride.model';

@Injectable()
export class ReservationStore {

    constructor(private http: HttpService,
                private requestSvc: RequestService) { }

    createReservation(reservation: BookIn): Observable<BookIn> {
        return this.http
            .put(this.requestSvc.getUrl('users/rides'), reservation, this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

    getReservationsDriver(): Observable<Ride[]> {
        return this.http
            .get(this.requestSvc.getUrl('users/me/rides/published'), this.requestSvc.generateOptions())
            .map((res: Response) => res.json())
            .catch((err: any) => Observable.throw(err));
    }

