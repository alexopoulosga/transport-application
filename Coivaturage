                                                                                                                                                                                                                                                                                                                                                                                                            descartesupem-app-457929343ebe/src/modules/ride/components/detail/detail.component.ts               0000664 0000000 0000000 00000010403 13304473505 0030615 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit, AfterViewInit } from '@angular/core';
import { registerLocaleData, Location } from '@angular/common';
import { FormGroup, FormControl, FormBuilder, Validators } from '@angular/forms';
import localeFr from '@angular/common/locales/fr';
import { ActivatedRoute, Router } from '@angular/router';
import { Observable } from 'rxjs/Observable';

import { Ride } from '@models/ride.model';
import { BookIn } from '@models/bookIn.model';
import { ShortBio } from '@models/shortbio.model';

import { Helpers } from '@shared/functions/helpers';

import { RideStore } from '@stores/ride.store';
import { ChatStore } from '@stores/chat.store';

import { ReservationStore } from '@stores/reservation.store';
import { ToasterService } from '@services/toaster.service';
import { DataService } from '@services/data.service';
import { AuthService } from '@services/auth.service';

import { getDriverBio } from '@user/components/shortbio/shortbio.component';

@Component({
    selector: 'md-detail',
    templateUrl: './detail.component.html',
    styleUrls: ['./detail.component.scss']
})
export class DetailComponent implements OnInit {
    id: number;

    ride: Ride;
    bio: ShortBio;
    bookData: BookIn = <BookIn>{};

    show = false;
    isMyRide: boolean;

    reservationForm: FormGroup;

    loaded = false;
    showLightbox = false;

    constructor(private route: ActivatedRoute,
                private router: Router,
                private rideStore: RideStore,
                private chatStore: ChatStore,
                private reservationStore: ReservationStore,
                private toasterSvc: ToasterService,
                private dataSvc: DataService,
                private authSvc: AuthService,
                private builder: FormBuilder,
                private location: Location) {
        registerLocaleData(localeFr);
        this._initForm();
    }

    ngOnInit() {
        const id: Observable<number> = this.route.params.map(param => param.id);
        id.subscribe((parameter: number) => {
            if (parameter) {
                this.id = +parameter;
                this.getRide(this.id);
            }
        });
    }

    showForm() {
        this.show = true;
    }

    openLightbox() {
        this.showLightbox = true;
    }

    closeLightbox() {
        this.showLightbox = false;
    }

    goBack() {
        this.location.back();
    }

    chat() {
        this.chatStore.chatExists(this.ride.d_id).subscribe(
            chatId => this.router.navigate(['/chat', chatId]),
            err => this.createChat(this.ride.d_id)
        );
    }

    createChat(driver: number) {
        this.chatStore.createChat(driver).subscribe(
            data => this.router.navigate(['/chat', data]),
            error => this.toasterSvc.error('Erreur lors de la crŒì¬©ation de la conversation')
        );
    }

    book() {
        if (this.reservationForm.valid) {
            this.bookData = {
                nb_person: this.reservationForm.value.passengers,
                ride_id: this.id
            };
            this.reserve();
        }
    }

    reserve() {
        this.reservationStore.createReservation(this.bookData).subscribe(
            success => {
                this.toasterSvc.success('Trajet rŒì¬©servŒì¬©');
                this.router.navigate(['reservation/passenger']);
            },
            error => this.toasterSvc.error('Vous ne pouvez plus rŒì¬©server ce trajet')
        );
    }

    getRide(id: number) {
        this.rideStore.getRide(id)
            .subscribe(
                data => {
                    this.ride = data;
                    this.bio = getDriverBio(this.ride);
                    this._setValidators();
                    this.isMyRide = data.d_id === this.authSvc.getUserId();
                },
                error => this.router.navigate(['error', error.status]),
                () => this.loaded = true
            );
    }

    private _setValidators() {
        this.reservationForm.get('passengers').setValidators([
            Validators.required,
            Validators.min(1),
            Validators.max(this.ride.free_place)
        ]);
    }

    private _initForm() {
        this.reservationForm = this.builder.group({
            passengers: [1, [Validators.required, Validators.min(1)]]
        });
    }

}
