        <nav class="lightbox-nav navigation">
            <ul>
                <li>
                    <a (click)="closeLightbox()">
                        <span class="wave wave-icons"></span>
                        <i class="material-icons">close</i>
                    </a>
                </li>
            </ul>
        </nav>
        <hr class="hr">
        <div class="lightbox-inner wrapper">
            <md-filter (filterEvent)="closeLightbox()" [params]="data"></md-filter>
        </div>
    </div>

</div>
