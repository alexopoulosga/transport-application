  templateUrl: './resetPassword.component.html',
  styleUrls: ['./resetPassword.component.scss']
})
export class ResetPasswordComponent {

    resetForm: FormGroup;

    email: string;

    constructor(private builder: FormBuilder,
                private authStore: AuthStore,
                private router: Router,
                private toasterSvc: ToasterService) {
        this._initForm();
    }

    resetPassword() {
        if (this.resetForm.valid) {
            this.email = this.resetForm.value;
            this.authStore.askForNewPassword(this.email)
                .subscribe(
                    data => {
                        this.toasterSvc.success('Un mail vous a Œì¬©tŒì¬© envoyŒì¬© pour renouveler votre mot de passe');
                        this.router.navigate(['/auth/signin']);
                    },
                    error => this.toasterSvc.error('Erreur lors de l\'envoi du mot de passe')
                );
        }
    }

    private _initForm() {
        this.resetForm = this.builder.group({
            email: ['', [Validators.required, Validators.email]]
        });
    }

}
