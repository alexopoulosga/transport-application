                                                                                                                                                                                                                                                                                                                                                                                                            descartesupem-app-457929343ebe/src/modules/ride/components/filter/filter.component.ts               0000664 0000000 0000000 00000005372 13304473505 0030674 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit, Output, Input, EventEmitter } from '@angular/core';
import { FormBuilder, FormGroup, Validators } from '@angular/forms';

import { CustomValidators } from '@validators/custom.validator';

import { DataService } from '@services/data.service';

import * as moment from 'moment';

@Component({
    selector: 'md-filter',
    templateUrl: './filter.component.html',
    styleUrls: ['./filter.component.scss']
})
export class FilterComponent implements OnInit {

    @Input() params;
    @Output() filterEvent: EventEmitter<boolean> = new EventEmitter();

    filterForm: FormGroup;

    radiusOptions: any;

    now: string;
    future: string;

    data: any;

    constructor(private builder: FormBuilder,
                private dataSvc: DataService) {
        this.now = moment().format('YYYY-MM-DD');
        this.future = moment().add(1, 'y').format('YYYY-MM-DD');
    }

    ngOnInit() {
        this.radiusOptions = this.params.radiusOptions;
        this._initForm();
    }

    search() {
        if (this.filterForm.value) {
            const values = this.filterForm.value;
            this.data = {
                places: [this.params.places[0], this.params.places[1]],
                passengers: values.passengers,
                datetime: {
                    date: values.datetime.date,
                    hours: values.datetime.hours,
                    minutes: values.datetime.minutes,
                    end: values.datetime.end
                },
                radius: values.radius,
                radiusOptions: this.radiusOptions,
                options: {
                    smoking: values.options.smoking,
                    food: values.options.food,
                    luggage: values.options.luggage
                }
            };
            this.dataSvc.update(this.data);
            this.filterEvent.emit(true);
        }
    }

    private _initForm() {
        this.filterForm = this.builder.group({
            datetime: this.builder.group({
                date: [this.params.datetime.date, [Validators.required, CustomValidators.notInThePastDateInput, CustomValidators.lessThanOneYear]],
                hours: [this.params.datetime.hours, Validators.required],
                minutes: [this.params.datetime.minutes, Validators.required],
                end: null
            }, { validator: CustomValidators.notInThePast}),
            passengers: [this.params.passengers, [Validators.required, Validators.min(1)]],
            radius: [this.params.radius, [Validators.required, Validators.min(this.radiusOptions.min), Validators.max(this.radiusOptions.max)]],
            options: this.builder.group({
                smoking: null,
                food: null,
                luggage: null
            })
        });
    }

}
