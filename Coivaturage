        <h2 class="heading mb-2">Demande de nouveau mot de passe :</h2>
        <div class="form-group mb-2">
            <input type="email" id="email" name="email" formControlName="email" placeholder="Email" autocomplete="off" required />
        </div>

        <div class="form-group mb-2">
            <button class="button button--green" type="submit" [disabled]="resetForm.invalid"><span class="wave wave-buttons"></span>Envoyer</button>
        </div>
        <p class="mt-3 align-center"><a class="link link--green" [routerLink]="['/', 'auth', 'signin']">Retour au login</a></p>
    </form>
