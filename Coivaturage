    <h2 class="heading mt-2 mb-2 wrapper">Rechercher un trajet :</h2>

    <form [formGroup]="searchForm" class="wrapper form pb-3" (submit)="continue()">
        <div class="form-group mb-2">
            <div class="form-group">
                <div formGroupName="places" class="search-ride-input has-geoloc">
                    <span class="split"></span>
                    <input type="text" class="ride-departure" id="departure" autocomplete="off" autocapitalize="off" autocorrect="off" spellcheck="false" placeholder="DŒì¬©part" formControlName="departure" #departure>
