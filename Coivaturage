                                                                                                                                                                                                                                                                                                                                                                                              descartesupem-app-457929343ebe/src/modules/app/components/tutorial/tutorial.component.ts            0000664 0000000 0000000 00000001643 13304473505 0031462 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit } from '@angular/core';
import { Router } from '@angular/router';

import { AuthService } from '@services/auth.service';

import { UserStore } from '@stores/user.store';

@Component({
    selector: 'md-tutorial',
    templateUrl: './tutorial.component.html',
    styleUrls: ['./tutorial.component.scss']
})
export class TutorialComponent implements OnInit {

    constructor(private authSvc: AuthService,
                private userStore: UserStore,
                private router: Router) { }

    ngOnInit() {
        if (this.authSvc.tutorialDone()) {
            this.router.navigate(['user']);
            return;
        }
    }

    done() {
        this.userStore.setTutorialState()
            .subscribe(
                success => this.router.navigate(['user']),
                error => console.log('err', error),
                () => this.router.navigate(['user'])
            );
    }

}
