                                                                                                                                                                                                                                                                                                                                                   descartesupem-app-457929343ebe/src/modules/ride/components/confirm/confirm.component.ts             0000664 0000000 0000000 00000014555 13304473505 0031217 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, ViewChild, OnInit, OnDestroy } from '@angular/core';
import { Router } from '@angular/router';
import { registerLocaleData } from '@angular/common';
import { FormGroup, Validators, FormBuilder } from '@angular/forms';

import { ToasterService } from '@services/toaster.service';
import { DataService } from '@services/data.service';
import { MapService } from '@services/map.service';
import { RidePost } from '@models/ridePost.model';
import { Place } from '@models/place.model';
import { Helpers } from '@functions/helpers';

import * as moment from 'moment';
import { Subscription } from 'rxjs/Subscription';

import { MapsAPILoader } from '@agm/core';
import { } from 'googlemaps';

import 'rxjs/add/operator/take';

import localeFr from '@angular/common/locales/fr';

import { RideStore } from '@stores/ride.store';


@Component({
    selector: 'md-confirm',
    templateUrl: './confirm.component.html',
    styleUrls: ['./confirm.component.scss']
})
export class ConfirmComponent implements OnInit, OnDestroy {

    data: any;
    ride: RidePost = <RidePost>{};

    confirmForm: FormGroup;

    subscription: Subscription;

    distance: number;
    duration: number;
    arrival_date: string;

    success = false;
    error = false;

    points: number;

    constructor(private dataSvc: DataService,
                private toasterSvc: ToasterService,
                private router: Router,
                private mapsAPI: MapsAPILoader,
                private builder: FormBuilder,
                private rideStore: RideStore,
                private mapSvc: MapService) {
        registerLocaleData(localeFr);
    }

    ngOnInit() {
        this._initForm();
        this.getPlaces();

        this.listenChanged();
    }

    getPlaces() {
        this.subscription = this.dataSvc.data.take(1).subscribe(
            data => this.data = data ? data : undefined,
            err => console.log(err),
            () => this._isValid()
        );
    }

    private _isValid() {
        if (this.data === undefined || this.data.places[0] === null || this.data.places[1] === null) {
            this.router.navigate(['/ride/new']);
            return;
        }

        this.mapSvc.calculateDurationAndDistance(this.data.places[0], this.data.places[1]);
    }

    listenChanged() {
        this.mapSvc.durationChanged.subscribe(
            success => {
                this._setForm();
                this.setPrice(this.mapSvc.getDistance());
            }
        );
    }

    saveRide() {
        if (this.confirmForm.valid) {
            this._prepareSave(this.confirmForm.value);
            this.rideStore.createRide(this.ride)
                .subscribe(
                    success => this.toasterSvc.success('Trajet ajoutŒì¬©'),
                    error => this.toasterSvc.error('Erreur lors de l\'ajout du trajet'),
                    () => this.router.navigate(['ride/search'])
                );
        }
    }

    setPrice(distance: number) {
        this.points = Math.ceil(Helpers.log3(distance) * 1.5 + distance);
    }

    private _prepareSave(values: any) {
        this.ride.car_id = values.carId;
        this.ride.nb_passenger = values.passengers;
        this.ride.date = `${values.datetime.date} ${values.datetime.time}:00`;
        this.ride.kilometers = values.distance;
        this.ride.duration = values.duration;
        this.ride.meeting_place = values.meetingPlace;
        this.ride.arrival_place = values.arrivalPlace;
    }

    private _initForm() {
        this.confirmForm = this.builder.group({
            carId: ['', Validators.required],
            passengers: ['', Validators.required],
            datetime: this.builder.group({
                date: ['', Validators.required],
                time: ['', Validators.required]
            }),
            distance: ['', [Validators.required, Validators.min(0.1)]],
            duration: ['', [Validators.required, Validators.min(1)]],
            details: ['', [Validators.minLength(0), Validators.maxLength(150)]],
            meetingPlace: this.builder.group({
                place: '',
                street: '',
                city: ['', Validators.required],
                postal_code: ['', Validators.required],
                latitude: ['', Validators.required],
                longitude: ['', Validators.required],
            }),
            arrivalPlace: this.builder.group({
                place: '',
                street: '',
                city: ['', Validators.required],
                postal_code: ['', Validators.required],
                latitude: ['', Validators.required],
                longitude: ['', Validators.required],
            })
        });
    }

    private _setForm() {
        const departure = this.data.places[0];
        const arrival = this.data.places[1];

        this.distance = this.mapSvc.getDistance();
        this.duration = this.mapSvc.getDuration();

        this.calculateArrivalTime();

        this.confirmForm.setValue({
            carId: this.data.car.id,
            passengers: this.data.passengers,
            datetime: {
                date: this.data.datetime.date,
                time: `${this.data.datetime.hours}:${this.data.datetime.minutes}`
            },
            distance: this.distance,
            duration: this.duration,
            details: this.data.details,
            meetingPlace: {
                place: departure.place ? departure.place : '',
                street: departure.street ? departure.street : '',
                city: departure.city,
                postal_code: departure.postal_code,
                latitude: departure.latitude,
                longitude: departure.longitude
            },
            arrivalPlace: {
                place: arrival.place ? arrival.place : '',
                street: arrival.street ? arrival.street : '',
                city: arrival.city,
                postal_code: arrival.postal_code,
                latitude: arrival.latitude,
                longitude: arrival.longitude
            }
        });

    }

    calculateArrivalTime() {
        const departure = moment(this.data.datetime.date + ' ' + this.data.datetime.hours + ':' + this.data.datetime.minutes, 'YYYY-MM-DD hh:mm');
        const arrival = departure.add(this.duration, 'm');
        this.arrival_date = arrival.format('YYYY-MM-DD HH:mm:ss');
    }

    ngOnDestroy() {
        this.subscription.unsubscribe();
        this.mapSvc.reset();
    }

}
