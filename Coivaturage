                                                                                                                                                                                                                                                                                                                                                                                       descartesupem-app-457929343ebe/src/modules/app/components/navbar/top/topNavBar.component.ts         0000664 0000000 0000000 00000002302 13304473505 0031714 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, Output, OnInit, OnDestroy } from '@angular/core';
import { Subscription } from 'rxjs/Subscription';

import { Helpers } from '@shared/functions/helpers';
import { NotificationStore } from '@stores/notification.store';


import { DataService } from '@services/data.service';
import { swipe } from '@shared/enums/swipe.eum';

@Component({
    selector: 'md-top-nav-bar',
    templateUrl: './topNavBar.component.html',
    styleUrls: ['./topNavBar.component.scss']
})
export class TopNavBarComponent implements OnInit, OnDestroy {

    showMenu = false;
    notifCount = 0;

    subscription: Subscription;

    constructor(private notifStore: NotificationStore, private dataSvc: DataService) { }

    ngOnInit() {
        this.subscription = this.dataSvc.data.subscribe(data => {
            if (data === swipe.left && !this.showMenu) {
                this.sidebarToggle();
            }
        });
        this.countNotif();
    }

    sidebarToggle() {
        this.showMenu = !this.showMenu;
    }

    countNotif() {
        this.notifStore.getCount().subscribe(
            data => this.notifCount = +data
        );
    }

    ngOnDestroy() {
        this.subscription.unsubscribe();
    }
}
