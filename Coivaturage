                                                                                                                                                                                                                                                                                                                                                                                                     descartesupem-app-457929343ebe/src/modules/ride/components/results/results.component.ts             0000664 0000000 0000000 00000005455 13304473505 0031326 0                                                                                                    ustar 00root                            root                            0000000 0000000                                                                                                                                                                        import { Component, OnInit, OnDestroy } from '@angular/core';
import { Router } from '@angular/router';
import { Location } from '@angular/common';

import { Observable } from 'rxjs/Observable';
import { Subscription } from 'rxjs/Subscription';
import 'rxjs/add/operator/take';

import { ShortBio } from '@shared/models/shortbio.model';
import { RideSearch } from '@models/rideSearch.model';
importŒí¬†{ Ride } from '@models/ride.model';

import { DataService } from '@services/data.service';

import { Helpers } from '@shared/functions/helpers';

import { RideStore } from '@stores/ride.store';

import { getDriverBio } from '@user/components/shortbio/shortbio.component';

@Component({
    selector: 'md-results',
    templateUrl: './results.component.html',
    styleUrls: ['./results.component.scss']
})
export class ResultsComponent implements OnInit, OnDestroy {
    bio: ShortBio;

    subscription: Subscription;

    data: any;
    ride: RideSearch = <RideSearch>{};

    rides: Ride[];

    loaded = false;
    showLightbox = false;

    constructor(private dataSvc: DataService,
                private router: Router,
                private rideStore: RideStore,
                private location: Location) { }

    ngOnInit() {
        this.getData();
    }

    getData() {
        this.subscription = this.dataSvc.data.subscribe(
            data => {
                this.data = data ? data : undefined;
                this._isValid();
            },
            err => console.log(err)
        );
    }

    goBack() {
        this.location.back();
    }

    openLightbox() {
        this.showLightbox = true;
    }

    closeLightbox() {
        this.showLightbox = false;
    }

    search() {
        this.rideStore.searchRides(this.ride).subscribe(
            rides => this.rides = rides,
            err => console.log('err', err),
            () => this.loaded = true
        );
    }

    convertRideToBio(ride: Ride) {
        return getDriverBio(ride);
    }

    ngOnDestroy() {
        this.subscription.unsubscribe();
    }

    private _prepareSearch(data: any) {
        const date = `${data.datetime.date} ${data.datetime.hours}:${data.datetime.minutes}:00`;
        this.ride.datestart = data.datetime.end ? null : date;
        this.ride.dateend = data.datetime.end ? date : null;
        this.ride.nb_passenger = data.passengers;
        this.ride.m_latitude = data.places[0].latitude,
        this.ride.m_longitude = data.places[0].longitude;
        this.ride.a_latitude = data.places[1].latitude;
        this.ride.a_longitude = data.places[1].longitude;
        this.ride.radius = +data.radius;

        this.search();
    }

    private _isValid() {
        if (this.data === undefined) {
            this.router.navigate(['/ride/search']);
            return;
        }
        this._prepareSearch(this.data);
    }
}
